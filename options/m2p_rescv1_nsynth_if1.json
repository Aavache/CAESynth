{
    "experiment_name": "m2p_rescv1_nsynth_if1",
    "data": {
        "dataset": "nsynth",
        "data_path": "./data/nsynth/nsynth-train",
        "meta_file": "examples.json",
        "inst": ["mallet","string","brass","reed", "flute"],
        "phase_format": "if",
        "mag_format": "log",
        "n_fft": 512,
        "hop": 256,
        "win_len": 512,
        "pitch_size": 128,
        "sample_size":1,
        "segment_size": 64000
    },
    "model": {
        "name": "mag2phase",
        "m2p": "rescv1",
        "in_ch": 2,
        "in_size": 256,
        "out_size": 256
    },
    "train": {
        "n_epochs": 100000,
        "iter_per_epoch": 2000,
        "start_epoch": 1,
        "lr": 1e-4,
        "beta1": 0.9,
        "save_epoch_freq": 10,
        "lambda_recon": 0.0,
        "lambda_gd": 0.0,
        "lambda_if": 1.0,
        "devices": [0],
        "batch_size": 1,
        "print_freq": 50,
        "save_latest_freq": 5000, 
        "epoch_shuffle": true,
        "batch_shuffle": true,
        "seed": 1234,
        "n_threads": 1,
        "recon_mode": "von-mises",
        "checkpoints_dir": "checkpoints"
    },
    "test": {
        "output_dir": "test_samples",
        "batch_shuffle": false,
        "devices": [0],
        "print_freq": 10,
        "wav_length": 64000,
        "n_threads": 1,
        "sample_size": 50,
        "batch_size": 1,
        "load_suffix": "latest",
        "seed": 1234
    }
}
