{
    "experiment_name": "aepclass_nsynth_recon10_class1",
    "data": {
        "dataset": "nsynth",
        "data_path": "./data/nsynth/nsynth-train",
        "meta_file": "examples.json",
        "inst": ["mallet","string","brass","reed", "flute"],
        "mag_format": "log",
        "n_fft": 2048,
        "hop": 512,
        "win_len": 2048, 
        "pitch_size": 128,
        "sample_size":1,
        "segment_size": 64000
    },
    "model": {
        "name": "aepclass",
        "ae": "gansynth_3",
        "in_ch": 1,
        "timbre_latent_size": 256
    },
    "train": {
        "n_epochs": 100000,
        "iter_per_epoch": 2000,
        "start_epoch": 1,
        "lr": 1e-4,
        "beta1": 0.9,
        "save_epoch_freq": 10,
        "lambda_recon": 10.0,
        "lambda_class": 1.0,
        "devices": [0],
        "batch_size": 1,
        "print_freq": 50,
        "save_latest_freq": 5000, 
        "epoch_shuffle": true,
        "batch_shuffle": true,
        "seed": 1234,
        "n_threads": 1,
        "recon_mode": "l1",
        "checkpoints_dir": "checkpoints"
    },
    "test": {
        "output_dir": "test_samples",
        "devices": [0],
        "print_freq": 10,
        "n_threads": 1,
        "batch_size": 1,
        "load_suffix": "latest",
        "seed": 1234
    }
}
