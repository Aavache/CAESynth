{
    "experiment_name": "progae_growgansynth3_if_nsynth_kl01_recon1_zrecon100",
    "data": {
        "dataset": "nsynth",
        "data_path": "./data/nsynth/nsynth-train",
        "meta_file": "examples.json",
        "inst": ["mallet","string","brass","reed", "flute"],
        "phase_format": "if",
        "pitch_size": 128,
        "sample_size": 1,
        "segment_size": 64000
