        "start_epoch": 1,
        "lr": 1e-4,
        "beta1": 0.9,
        "save_epoch_freq": 10,
        "epoch_shuffle": true,
        "batch_shuffle": true,
        "seed": 1234,
        "devices": [0],
        "n_threads": 1,
        "batch_size": 1,
        "print_freq": 50,
        "save_latest_freq": 5000, 
