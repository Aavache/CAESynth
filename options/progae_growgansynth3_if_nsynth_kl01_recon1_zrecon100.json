        "lambda_recon": 1.0,
        "lambda_f_kl_max": 0.1,
        "lambda_f_kl_min": 0.1,
        "lambda_latent_recon": 100.0,
        "recon_mode": "l1",
        "recon_embed_mode": "cos",
        "checkpoints_dir": "checkpoints"
    },
    "test": {
        "output_dir": "test_samples",
        "batch_shuffle": false,
        "devices": [0],
        "print_freq": 10,
        "wav_length": 64000,
        "n_threads": 1,
        "sample_size": 50,
        "batch_size": 1,
        "load_suffix": "latest",
        "seed": 1234
    }
